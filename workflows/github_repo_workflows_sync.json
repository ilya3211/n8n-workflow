      "parameters": {
        "jsCode": "const results = [];\n\nfor (const item of $input.all()) {\n    // Deep copy using JSON\n    const workflow = JSON.parse(JSON.stringify(item.json));\n\n    // Remove auto-generated fields from n8n\n    const fieldsToRemove = [\n        'id', 'createdAt', 'updatedAt', 'versionId',\n        'meta', 'pinData', 'staticData', 'shared',\n        'isArchived', 'triggerCount'\n    ];\n\n    fieldsToRemove.forEach(field => {\n        delete workflow[field];\n    });\n\n    // Clean tags - keep only name\n    if (workflow.tags && Array.isArray(workflow.tags)) {\n        workflow.tags = workflow.tags.map(tag => {\n            if (typeof tag === 'object' && tag.name) {\n                return { name: tag.name };\n            }\n            return tag;\n        });\n    }\n\n    results.push({ json: workflow });\n}\n\nreturn results;"
      },
